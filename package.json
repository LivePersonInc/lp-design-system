{
  "name": "@liveperson/styled-components",
  "description": "Styled components",
  "version": "1.0.0",
  "license": "private",
  "private": true,
  "main": "build/direflowBundle.js",
  "files": [
    "build"
  ],
  "scripts": {
    "start": "start-storybook -p 6006 -s public",
    "build": "direflow-scripts build && node ./scripts/group-icons-components.js",
    "build:lib": "direflow-scripts build:lib",
    "test": "direflow-scripts test",
    "build-storybook": "yarn build && node ./scripts/move-direflowBuild.js && build-storybook -s public  && node ./scripts/remove-direflowBuild.js"
  },
  "dependencies": {
    "@types/node": "12.7.8",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "classnames": "^2.3.1",
    "direflow-component": "3.5.5",
    "direflow-scripts": "3.5.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/node-logger": "^6.3.6",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.6",
    "@types/jest": "^26.0.24",
    "@types/react-test-renderer": "16.9.1",
    "css-loader": "3.4.2",
    "fs-extra": "^10.0.0",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "react-app-rewired": "2.1.3",
    "react-test-renderer": "16.9.0",
    "sass-loader": "8.0.2",
    "style-loader": "0.23.1",
    "to-string-loader": "^1.1.6",
    "ts-loader": "^6.2.2",
    "tslint": "5.20.0",
    "tslint-config-airbnb": "5.11.2",
    "tslint-react": "4.1.0",
    "typescript": "^3.9.10",
    "webpack-cli": "^3.3.12"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "direflow-scripts/direflow-jest.config.js"
    ]
  },
  "config-overrides-path": "direflow-webpack.js"
}
